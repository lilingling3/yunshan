{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Partner</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Secret</th>
                <td>{{ entity.secret }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Createtime</th>
                <td>{{ entity.createTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ entity.amount }}</td>
            </tr>
            <tr>
                <th>Actureamount</th>
                <td>{{ entity.actureAmount }}</td>
            </tr>
            <tr>
                <th>Discount</th>
                <td>{{ entity.discount }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('auto_admin_partner_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('auto_admin_partner_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
